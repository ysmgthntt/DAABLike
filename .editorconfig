[*.cs]

# CS8604: Null 参照引数の可能性があります。
dotnet_diagnostic.CS8604.severity = error

# CS8625: null リテラルを null 非許容参照型に変換できません。
dotnet_diagnostic.CS8625.severity = error

# CA1068: CancellationToken パラメーターは最後に指定する必要があります
dotnet_diagnostic.CA1068.severity = error

# CSIsNull001: Use `is null` for null checks
dotnet_diagnostic.CSIsNull001.severity = error

# CSIsNull002: Use `is object` for non-null checks
dotnet_diagnostic.CSIsNull002.severity = error
